{# common html body elements for ZabbixReports #}
<htmlpagefooter name="defaultReportFooter">
<div class="footer">
	<div class="footer-logo">generated by <img class="footer-logo-img" src="../images/logo/zabbixreports_logo_100x12.png" /></div>
	<div class="footer-pageno">page {PAGENO}</div>
</div>
</htmlpagefooter>

<htmlpageheader name="defaultReportHeader">
<div class="header">
	<div class="header-title">{{ REPORT_TITLE }}</div>
	<div class="header-date">{{ "now"|date(timeformat) }}</div>
</div>
</htmlpageheader>
